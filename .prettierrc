{
   "tabWidth": 3,
   "printWidth": 100,
   "semi": false,
   "singleQuote": true,
   "arrowParens": "avoid",
   "trailingComma": "es5",
   "plugins": ["./node_modules/@trivago/prettier-plugin-sort-imports"],
   "importOrderSeparation": true,
   "importOrderSortSpecifiers": true,
   "importOrder": [
      "<THIRD_PARTY_MODULES>",

      "^dotenv/config$",
      "^./config/aliases$",
      "^config$",

      "^database(.*)",

      "^shared$",

      "^middlewares(.*)",

      "^routes(.*)",

      "^redux/hooks$",
      "^redux/store$",
      "^redux/reducers(.*)",

      "^styles(.*)",

      "^assets(.*)",

      "(.*)/styled$",

      "^components/shared$",
      "^components(.*)",
      "^./components$",

      "^./modules$",

      "^hooks$",
      "(.*)/hooks$",

      "^helpers$",
      "(.*)/helpers$",

      "^utils$",
      "(.*)/utils$",

      "^types(.*)",
      "(.*)/types$",

      "^[./]"
   ]
}
